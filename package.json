    "check:surgeons": "tsx scripts/check-duplicate-surgeons.ts",
    "health:check": "./scripts/health-check.sh",
    "prepare": "husky install",
    "test:security": "npm audit && npm run test:critical",
    "audit:debt": "./scripts/audit-technical-debt.sh",
    "quality:full": "npm run test:critical && npm run perf:audit",
    "storybook": "storybook dev -p 6006",
    "test:manual": "node scripts/automated-manual-tester-complete.js",
    "test:manual:headless": "HEADLESS=true node scripts/automated-manual-tester-complete.js",
    "test:manual:dev": "HEADLESS=false BASE_URL=http://localhost:3000 node scripts/automated-manual-tester-complete.js"

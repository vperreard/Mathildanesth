  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "./scripts/clean-browser-and-start.sh",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "test": "jest",
    "db:migrate": "prisma migrate dev",
